<?xml version="1.0" encoding="utf-8"?>


<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
android:shape="rectangle">

    <item>
        <shape>

            <stroke
                android:width="5.5dp"
                android:color="@color/primaryDarkColor" />

            <padding
                android:bottom="3.0dp"
                android:left="1.2dp"
                android:right="3.0dp"
                android:top="1.2dp" />

            <corners android:radius="50dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/color_background_list_item" />
            <corners android:radius="50dp" />
        </shape>
    </item>

</layer-list>